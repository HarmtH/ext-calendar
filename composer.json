{
	"name": "fisharebest/ext-calendar",
	"description": "Implementation of various calendars, plus PHP shims for the ext/calendar extension",
	"type": "library",
	"keywords": [
		"calendar", "ext-calendar", "gregorian", "julian", "jewish", "hebrew", "french", "julian day", "julian day number"
	],
	"homepage": "https://github.com/fisharebest/ext-calendar",
	"license": "GPL-3.0+",
	"authors": [{
			"name": "Greg Roach",
			"email": "fisharebest@gmail.com",
			"role": "Developer"
	}],
	"support": {
			"issues": "https://github.com/fisharebest/ext-calendar/issues"
	},
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"ext-calendar": "*",
		"ext-mbstring": "*",
		"phpunit/phpunit": "*",
		"satooshi/php-coveralls": "dev-master"
	},
	"suggest": {
		"patchwork/utf8": "ext/mb_string support is required to generate dates using Hebrew text."
	},
	"autoload": {
		"psr-4": {
			"Fisharebest\\ExtCalendar\\": "src/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"composer dump-autoload --optimize"
		],
		"post-update-cmd": [
			"composer dump-autoload --optimize"
		]
	}
}

