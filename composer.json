{
	"name": "fisharebest/ext-calendar",
	"description": "PHP shim for the calendar extension",
	"type": "library",
	"keywords": [
		"calendar", "ext-calendar",
		"jdtogregorian"
		],
	"homepage": "https://github.com/fisharebest/ext-calendar",
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Greg Roach",
			"email": "fisharebest@gmail.com",
						"role": "Developer"
		}
	],
	"support": {
			"issues": "https://github.com/fisharebest/ext-calendar/issues"
	},
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Fisharebest\\ExtCalendar\\": "src/"
		}
	},
	"scripts" : {
		"post-install-cmd": [
			"composer dump-autoload --optimize"
		],
		"post-update-cmd": [
			"composer dump-autoload --optimize"
		]
	}
}

