{
	"name": "fisharebest/ext-calendar",
	"description": "Implementation of the Arabic (Hijri), French, Gregorian, Jewish, Julian and Persian (Jalali) calendars.  Optional PHP shims for servers without the ext/calendar extension.",
	"type": "library",
	"keywords": [
		"calendar", "ext-calendar", "julian day", "julian day number",
		"arabic", "hijri", "persian", "jalali", "shamsi",
		"gregorian", "julian", "jewish", "hebrew", "french"
	],
	"homepage": "https://github.com/fisharebest/ext-calendar",
	"license": "GPL-3.0+",
	"authors": [{
			"name": "Greg Roach",
			"email": "fisharebest@gmail.com",
			"role": "Developer"
	}],
	"support": {
			"issues": "https://github.com/fisharebest/ext-calendar/issues"
	},
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Fisharebest\\ExtCalendar\\": "src/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"composer dump-autoload --optimize"
		],
		"post-update-cmd": [
			"composer dump-autoload --optimize"
		]
	}
}

